{
  "root": true,
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  },
  "extends": ["plugin:vue/recommended", "eslint:recommended", "prettier"],
  "parserOptions": {
    "parser": "@babel/eslint-parser"
  },
  "rules": {
    "prettier/prettier": "off",
    "vue/no-multiple-template-root": "off",
    "vue/attributes-order": "off",
    "vue/order-in-components": "off"
  }
}
